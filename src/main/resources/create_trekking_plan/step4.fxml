<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" prefWidth="284.0" xmlns="http://javafx.com/javafx/8.0.141">
    <children>
        <Label text="What kind of hotel will you stay?" textFill="#828282"/>
        <Rating fx:id="_ratingBar" rating="0.0" updateOnHover="true">
            <VBox.margin>
                <Insets top="24.0"/>
            </VBox.margin>
        </Rating>
        <JFXButton fx:id="_btnNext" prefHeight="38.0" prefWidth="190.0" ripplerFill="#ffffff42"
                   style="-fx-background-color: #1a535c; -fx-background-radius: 100;" text="NEXT" textFill="WHITE"
                   visible="false">
            <font>
                <Font size="10.0"/>
            </font>
            <padding>
                <Insets bottom="8.0" top="8.0"/>
            </padding>
            <VBox.margin>
                <Insets top="16.0"/>
            </VBox.margin>
        </JFXButton>
    </children>
</VBox>
