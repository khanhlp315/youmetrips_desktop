<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.*?>
<ScrollPane xmlns:fx="http://javafx.com/fxml/1" hbarPolicy="NEVER" stylesheets="@style/customScrollPane.css"
            xmlns="http://javafx.com/javafx/8.0.141">
    <content>
        <VBox prefWidth="412.0" style="-fx-background-color: f5f7fb;">
            <children>
                <VBox alignment="TOP_CENTER" style="-fx-background-color: white;">
                    <children>
                        <HBox>
                            <children>
                                <Rectangle fx:id="_rectAvatar" arcHeight="30.0" arcWidth="30.0" fill="DODGERBLUE"
                                           height="100.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0"
                                           width="100.0">
                                    <HBox.margin>
                                        <Insets left="32.0"/>
                                    </HBox.margin>
                                </Rectangle>
                                <VBox>
                                    <children>
                                        <HBox>
                                            <children>
                                                <Label fx:id="_lblFirstName" text="Firstname">
                                                    <font>
                                                        <Font name="System Bold" size="24.0"/>
                                                    </font>
                                                </Label>
                                                <Label fx:id="_lblLastName" text="Lastname">
                                                    <HBox.margin>
                                                        <Insets left="8.0"/>
                                                    </HBox.margin>
                                                    <font>
                                                        <Font size="24.0"/>
                                                    </font>
                                                </Label>
                                            </children>
                                        </HBox>
                                        <Label fx:id="_lblJob" text="Job" textFill="#787878">
                                            <graphic>
                                                <FontAwesomeIconView fill="#787878" glyphName="BRIEFCASE"/>
                                            </graphic>
                                            <VBox.margin>
                                                <Insets top="8.0"/>
                                            </VBox.margin>
                                        </Label>
                                        <Label fx:id="_lblHometown" text="Job" textFill="#787878">
                                            <graphic>
                                                <FontAwesomeIconView fill="#787878" glyphName="HOME"/>
                                            </graphic>
                                            <VBox.margin>
                                                <Insets top="8.0"/>
                                            </VBox.margin>
                                        </Label>
                                    </children>
                                    <padding>
                                        <Insets left="16.0" top="6.0"/>
                                    </padding>
                                </VBox>
                            </children>
                        </HBox>
                        <Label text="Bio.">
                            <VBox.margin>
                                <Insets top="16.0"/>
                            </VBox.margin>
                            <font>
                                <Font size="13.0"/>
                            </font>
                        </Label>
                        <JFXButton text="Edit" textFill="#1a535c">
                            <font>
                                <Font name="System Bold" size="12.0"/>
                            </font>
                        </JFXButton>
                    </children>
                    <padding>
                        <Insets bottom="8.0" top="10.0"/>
                    </padding>
                </VBox>
                <VBox alignment="TOP_CENTER" style="-fx-background-color: white;">
                    <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="10.0"/>
                    </padding>
                    <VBox.margin>
                        <Insets top="8.0"/>
                    </VBox.margin>
                    <children>
                        <HBox alignment="CENTER_LEFT">
                            <children>
                                <StackPane>
                                    <children>
                                        <Circle fill="#1a535c" radius="15.0" stroke="BLACK" strokeType="INSIDE"
                                                strokeWidth="0.0"/>
                                        <FontAwesomeIconView fill="WHITE" glyphName="PLANE" size="18">
                                            <StackPane.margin>
                                                <Insets top="2.0"/>
                                            </StackPane.margin>
                                        </FontAwesomeIconView>
                                    </children>
                                </StackPane>
                                <Label text="Your trekking plans">
                                    <HBox.margin>
                                        <Insets left="8.0"/>
                                    </HBox.margin>
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                </Label>
                            </children>
                        </HBox>
                        <JFXListView prefHeight="200.0" style="-fx-background-color: white;">
                            <VBox.margin>
                                <Insets top="10.0"/>
                            </VBox.margin>
                        </JFXListView>
                        <AnchorPane>
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                            <children>
                                <JFXButton fx:id="_btnUpdate" prefHeight="38.0" prefWidth="300.0"
                                           ripplerFill="#ffffff42"
                                           style="-fx-background-color: #1a535c; -fx-background-radius: 100;"
                                           text="Have a new plan" textFill="WHITE" AnchorPane.leftAnchor="0.0"
                                           AnchorPane.rightAnchor="0.0">
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                </JFXButton>
                                <AnchorPane prefHeight="1.0" prefWidth="200.0" style="-fx-background-color: #f0f0f0;"
                                            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                            AnchorPane.topAnchor="50.0"/>
                            </children>
                        </AnchorPane>
                        <JFXButton text="See All Plans    &gt;" textFill="#787878">
                            <VBox.margin>
                                <Insets top="4.0"/>
                            </VBox.margin>
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </JFXButton>
                    </children>
                </VBox>
                <VBox alignment="TOP_CENTER" style="-fx-background-color: white;">
                    <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="10.0"/>
                    </padding>
                    <children>
                        <HBox alignment="CENTER_LEFT">
                            <children>
                                <StackPane>
                                    <children>
                                        <Circle fill="#ed5851" radius="15.0" stroke="BLACK" strokeType="INSIDE"
                                                strokeWidth="0.0"/>
                                        <MaterialIconView fill="WHITE" glyphName="PEOPLE" size="18"/>
                                    </children>
                                </StackPane>
                                <Label text="Your friends">
                                    <HBox.margin>
                                        <Insets left="8.0"/>
                                    </HBox.margin>
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                </Label>
                                <Label fx:id="_lblFriendCount" text="0">
                                    <HBox.margin>
                                        <Insets left="16.0"/>
                                    </HBox.margin>
                                    <font>
                                        <Font name="System Bold" size="14.0"/>
                                    </font>
                                </Label>
                            </children>
                        </HBox>
                        <JFXListView orientation="HORIZONTAL" prefHeight="100.0" style="-fx-background-color: white;">
                            <VBox.margin>
                                <Insets top="10.0"/>
                            </VBox.margin>
                        </JFXListView>
                    </children>
                    <VBox.margin>
                        <Insets top="8.0"/>
                    </VBox.margin>
                </VBox>
            </children>
        </VBox>
    </content>
</ScrollPane>
