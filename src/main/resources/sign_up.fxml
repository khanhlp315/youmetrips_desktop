<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="490.0" prefWidth="338.0"
            xmlns="http://javafx.com/javafx/9.0.1">
    <children>
        <AnchorPane layoutX="59.0" layoutY="127.0" prefHeight="305.0" prefWidth="212.0"
                    style="-fx-background-color: white; -fx-background-radius: 10; -fx-effect: dropshadow(gaussian, #cf9d3c, 10, 0, 3, 3);">
            <children>
                <AnchorPane layoutX="11.0" layoutY="59.0">
                    <children>
                        <JFXTextField fx:id="_txtLastNameSignUp" focusColor="#ffba33" labelFloat="true" layoutX="119.0" layoutY="144.0"
                                      prefHeight="31.0" prefWidth="71.0" promptText="Last Name" unFocusColor="#ccd0d2"/>
                        <JFXTextField fx:id="_txtEmailOrPhoneSignUp" focusColor="#ffba33" labelFloat="true" layoutY="11.0" prefHeight="31.0"
                                      prefWidth="190.0" promptText="Email or Phone" unFocusColor="#ccd0d2"/>
                        <JFXPasswordField fx:id="_txtPasswordSignUp" focusColor="#ffba33" labelFloat="true" layoutY="56.0" prefHeight="31.0"
                                          prefWidth="190.0" promptText="Password" unFocusColor="#ccd0d2"/>
                        <JFXPasswordField fx:id="_txtConfirmPasswordSignUp" focusColor="#ffba33" labelFloat="true" layoutY="102.0" prefHeight="31.0"
                                          prefWidth="190.0" promptText="Confirm Password" unFocusColor="#ccd0d2"/>
                        <JFXTextField fx:id="_txtFirstNameSignUp" focusColor="#ffba33" labelFloat="true" layoutY="144.0" prefHeight="31.0"
                                      prefWidth="99.0" promptText="First Name" unFocusColor="#ccd0d2"/>
                    </children>
                </AnchorPane>
                <JFXButton onMouseClicked="#signUp" layoutX="11.0" layoutY="251.0" prefHeight="38.0" prefWidth="190.0" ripplerFill="#ffffff42"
                           style="-fx-background-color: #1a535c; -fx-background-radius: 100;" text="CREATE ACCOUNT"
                           textFill="WHITE">
                    <font>
                        <Font size="10.0"/>
                    </font>
                </JFXButton>
                <AnchorPane layoutX="1.0" layoutY="13.0" prefHeight="31.0" prefWidth="209.0"
                            style="-fx-effect: dropshadow(gaussian, #f7f1e6, 10, 0, 0, 3); -fx-background-color: white;">
                    <children>
                        <Label layoutX="14.0" layoutY="3.0" prefHeight="17.0" style="-fx-font-weight: bold;"
                               text="&lt; ALREADY A MEMBER" textFill="#ffc146">
                            <font>
                                <Font size="9.0"/>
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
