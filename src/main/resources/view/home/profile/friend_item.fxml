<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.141">
    <children>
        <Rectangle fx:id="_rectAvatar" arcHeight="30.0" arcWidth="30.0" fill="DODGERBLUE" height="100.0" stroke="BLACK"
                   strokeType="INSIDE" strokeWidth="0.0" width="100.0"/>
        <Label fx:id="_lblName" text="First Lastname">
            <font>
                <Font size="13.0"/>
            </font>
            <VBox.margin>
                <Insets top="4.0"/>
            </VBox.margin>
        </Label>
    </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
    </padding>
</VBox>
