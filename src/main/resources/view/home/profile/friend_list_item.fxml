<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141"
            fx:controller="view.profile.FriendListItem">
    <children>
        <HBox alignment="CENTER">
            <children>
                <Rectangle fx:id="_rectAvatar" arcHeight="30.0" arcWidth="30.0" fill="DODGERBLUE" height="80.0"
                           stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="80.0"/>
                <Label fx:id="_lblName" text="First Lastname">
                    <font>
                        <Font size="13.0"/>
                    </font>
                    <HBox.margin>
                        <Insets left="16.0"/>
                    </HBox.margin>
                </Label>
            </children>
            <padding>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </padding>
        </HBox>
    </children>
</AnchorPane>
