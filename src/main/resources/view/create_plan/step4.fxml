<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import org.controlsfx.control.Rating?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" prefWidth="284.0" xmlns="http://javafx.com/javafx/8.0.141">
    <children>
        <Label text="What kind of hotel will you stay?" textFill="#828282"/>
        <Rating fx:id="_ratingBar" rating="0.0" stylesheets="@../../style/ratingBar.css" updateOnHover="true">
            <VBox.margin>
                <Insets top="24.0"/>
            </VBox.margin>
        </Rating>
    </children>
</VBox>
